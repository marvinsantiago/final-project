
<div class='activites'>
    <h4>Popular Activities</h4>
    <ul>
        {{#each model.activities as |activity|}}
            <li>{{fa-icon 'fa-star-o'}} - {{activity.name}}</li>
        {{/each}}
    </ul>
</div>

<div class='events'>
    <h6>Upcoming Events</h6>
    <ul>
        {{#each model.events as |event|}}
            <li>{{event.name}}</li>
        {{/each}}
    </ul>
</div>

{{outlet}}

